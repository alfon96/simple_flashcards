{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "WSL Edge",
      "type": "msedge",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/*",
        "/app/*": "${workspaceFolder}/*"
      },
      "skipFiles": [
        "**/node_modules/**",
        "${workspaceFolder}/build/installHook.js"
      ]
    }
  ]
}
